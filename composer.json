{
    "name": "odinbi/pwa",
    "description": "odinbi pwa package",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/DuyRom/odinbi-pwa-package",
    "support": {
        "issues": "https://github.com/DuyRom/odinbi-pwa-package/issues",
        "source": "https://github.com/DuyRom/odinbi-pwa-package"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Odinbi\\Pwa\\": "src/"
        }
    },
    "authors": [
        {
            "name": "duynguyen",
            "email": "duyit192@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^7.1|^7.2|^7.3|^7.4|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Odinbi\\Pwa\\OdinbiPackageServiceProvider"
            ],
            "aliases": {
                "PWA": "Odinbi\\Pwa\\Facades\\PWA"
            }
        }
    }
}
